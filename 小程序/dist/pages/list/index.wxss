/*
    列表页样式
*/
page {
  position: relative;
  padding-top: 140rpx;
  padding-bottom: 120rpx;
  background-color: #FFF;
}
.search {
  position: fixed;
  top: 0;
  right: 0;
  left: 0;
  width: 100%;
  height: 55px;
  padding: 10px;
  background-color: #FFF;
  border-top: 1px solid #EAEDEE;
  border-bottom: 1px solid #EAEDEE;
  box-shadow: 0 0 20px 2rpx #EAEDEE;
  z-index: 9;
  box-sizing: border-box;
}
.search .search_input {
  position: relative;
  width: 100%;
  height: 36px;
  padding: 3px 45px 3px 35px;
  border-radius: 5px;
  color: #666;
  font-size: 28rpx;
  line-height: 2;
  box-sizing: border-box;
}
.search .search_input:after {
  content: ' ';
  position: absolute;
  top: 7px;
  left: 5px;
  width: 20px;
  height: 20px;
  background-image: url("data:image/png;base64,");
  background-size: contain;
}
.search .cancel {
  position: absolute;
  top: 36rpx;
  right: 30rpx;
  width: 50rpx;
  height: 50rpx;
  font-size: 28rpx;
  background-image: url("data:image/png;base64,");
  background-size: contain;
}
.menus {
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  background-color: rgba(0, 0, 0, 0.3);
  z-index: 99;
}
.menus .menus-content {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 90px;
  padding: 40rpx 0 0 30rpx;
  background-color: #FFF;
  z-index: 99;
  overflow: hidden;
}
.menus .menus-content .menus_select {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 60px;
  left: 30rpx;
  overflow: auto;
}
.menus .menus-content .menus_select .warp-box {
  margin-bottom: 30rpx;
}
.menus .menus-content .menus_select .warp-box .menus-title {
  position: relative;
  font-size: 32rpx;
  color: #333;
  font-weight: 700;
}
.menus .menus-content .menus_select .warp-box .menus-title .showmore {
  position: absolute;
  top: 5rpx;
  right: 90rpx;
  height: 10px;
  width: 10px;
  font-weight: 400;
  border-width: 4rpx 4rpx 4rpx 4rpx;
  border-color: transparent #9EA6AF #9EA6AF transparent;
  border-style: solid;
  transform: rotate(45deg);
}
.menus .menus-content .menus_select .warp-box .menus-list {
  padding-top: 28rpx;
  padding-right: 10rpx;
}
.menus .menus-content .menus_select .warp-box .menus-list.showmore {
  height: 270rpx;
  overflow: hidden;
}
.menus .menus-content .menus_select .warp-box .menus-list .menus-sub {
  position: relative;
  display: inline-block;
  min-width: 140rpx;
  max-width: 242rpx;
  margin-right: 20rpx;
  margin-bottom: 14rpx;
  padding: 0 20rpx;
  font-size: 24rpx;
  color: #7F858A;
  text-align: center;
  text-overflow: ellipsis;
  letter-spacing: 1rpx;
  white-space: nowrap;
  overflow: hidden;
  line-height: 2.7;
  border: 1px solid  #B7BBBF;
  border-radius: 10rpx;
  box-sizing: border-box;
}
.menus .menus-content .menus_select .warp-box .menus-list .menus-sub.active {
  border: 1px solid  #00AF99;
}
.menus .menus-content .menus_select .warp-box .menus-list .menus-sub.active:after {
  content: '';
  position: absolute;
  right: 0;
  bottom: 0;
  width: 34rpx;
  height: 34rpx;
  background-image: url("data:image/png;base64,");
  background-size: contain;
}
.menus .menus-content .menus_btn_ground {
  position: absolute;
  right: 0;
  bottom: 0;
  left: 0;
  box-shadow: 0 0 30px 0 rgba(0, 0, 0, 0.5);
  box-sizing: border-box;
}
.menus .menus-content .menus_btn_ground .menus_btn {
  display: inline-block;
  width: 50%;
  text-align: center;
  color: #333;
  font-size: 32rpx;
  line-height: 3;
  letter-spacing: 1rpx;
}
.menus .menus-content .menus_btn_ground .menus_btn.primary {
  background-color: #00A9FF;
  color: #FFF;
}
.list {
  position: relative;
  background-color: #FFF;
  overflow: auto;
  z-index: 2;
}
.list .result {
  padding: 20rpx 30rpx;
}
.list .result .num {
  display: inline-block;
  color: #00A9FF;
  font-size: 28rpx;
}
.list .result .text {
  display: inline-block;
  color: #939BA5;
  font-size: 28rpx;
}
.list .box {
  position: relative;
  width: 100%;
  height: 145rpx;
  margin: 0 0 50rpx;
  background-color: #FFF;
  overflow: hidden;
}
.list .box.hide {
  display: none;
}
.list .box .cover {
  position: relative;
  top: 6px;
  left: 30rpx;
  bottom: -1px;
  display: inline-block;
  width: 244rpx;
  height: 130rpx;
  border-radius: 5px;
  background-color: #FFF;
  overflow: hidden;
  z-index: 1;
}
.list .box .cover image {
  display: inline-block;
  width: 244rpx;
  vertical-align: top;
}
.list .box .content {
  position: absolute;
  top: 5rpx;
  left: 300rpx;
  right: 10px;
  bottom: 0;
}
.list .box .content .name {
  font-size: 28rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  text-overflow: ellipsis;
  word-break: break-all;
  overflow: hidden;
}
.list .box .content .info {
  position: absolute;
  left: 0;
  right: 0;
  bottom: 0;
  height: 20px;
  font-size: 24rpx;
  color: #666;
}
.list .box .content .info .user {
  position: absolute;
  top: 0;
  left: 0;
  width: 20%;
}
.list .box .content .info .number {
  position: absolute;
  top: 0;
  left: 25%;
}
.list .box .content .info .price {
  position: absolute;
  left: 0;
  top: 0;
  font-size: 16px;
  color: #E91D00;
}
.list .box .content .info .price.free {
  color: #00C08B;
}
.list .box .content .info .price .del {
  position: relative;
  top: 3px;
  display: inline-block;
  padding-left: .5em;
  font-size: 24rpx;
  color: #939BA5;
  text-decoration: line-through;
}
