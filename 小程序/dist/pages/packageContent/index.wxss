.wxParse-u {
  text-decoration: underline;
}
.wxParse-hide {
  display: none;
}
.WxEmojiView {
  align-items: center;
}
.wxEmoji {
  width: 16px;
  height: 16px;
}
.wxParse-tr {
  display: flex;
  border-right: 1px solid #e0e0e0;
  border-bottom: 1px solid #e0e0e0;
  border-top: 1px solid #e0e0e0;
}
.wxParse-th,
.wxParse-td {
  flex: 1;
  padding: 5px;
  font-size: 28rpx;
  border-left: 1px solid #e0e0e0;
  word-break: break-all;
}
.wxParse-td:last {
  border-top: 1px solid #e0e0e0;
}
.wxParse-th {
  background: #f0f0f0;
  border-top: 1px solid #e0e0e0;
}
.wxParse-del {
  display: inline;
}
.wxParse-figure {
  overflow: hidden;
}
page {
  background-color: #FFF;
}
.banner {
  position: relative;
  width: 100%;
  border-bottom: 1px solid  #DDD;
}
.banner .images {
  width: 100%;
  overflow: hidden;
}
.banner .images image {
  width: 100%;
}
.banner .tips {
  position: absolute;
  right: 0;
  bottom: 0;
  left: 0;
  height: 50px;
  background-color: rgba(0, 0, 0, 0.6);
  display: none;
}
.banner .player {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 100%;
}
.class-content .swiper-tab {
  position: relative;
  width: 100%;
  height: 50px;
  background-color: #FFF;
  overflow: hidden;
}
.class-content .swiper-tab:after {
  content: '';
  position: absolute;
  top: 30%;
  left: 50%;
  bottom: 30%;
  width: 0;
  border-left: 1px solid #DDD;
}
.class-content .swiper-tab:before {
  content: '';
  position: absolute;
  right: 0;
  bottom: 2rpx;
  left: 0;
  height: 0;
  border-bottom: 1px solid #DDD;
}
.class-content .swiper-tab .swiper-tab-list {
  position: relative;
  display: inline-block;
  width: 50%;
  height: 50px;
  font-size: 32rpx;
  line-height: 50px;
  text-align: center;
  letter-spacing: 1rpx;
  font-weight: 700;
  box-sizing: border-box;
  overflow: hidden;
}
.class-content .swiper-tab .swiper-tab-list.active {
  color: #00A9FF;
}
.class-content .swiper-tab .swiper-tab-list.active:after {
  content: '';
  position: absolute;
  left: 50%;
  bottom: 0;
  width: 1em;
  margin-left: -0.5em;
  border-bottom: 3px solid #00A9FF;
}
.class-content .swiper-contents {
  background-color: #F2F4F6;
}
.class-content .swiper-contents .swiper-contents-tabs .part {
  margin-bottom: 20rpx;
  padding: 10px 10px 0;
  background-color: #FFF;
}
.class-content .swiper-contents .swiper-contents-tabs .part:last-child {
  margin-bottom: 0;
}
.class-content .swiper-contents .swiper-contents-tabs .video {
  background-color: #FFF;
  padding: 10px 30rpx 0;
}
.class-content .swiper-contents .class-type {
  width: 100%;
  height: 86rpx;
  background-color: #FFF;
}
.class-content .swiper-contents .class-type .type-sub {
  position: relative;
  display: inline-block;
  width: 33.33%;
  height: 86rpx;
  text-align: center;
  font-size: 28rpx;
  line-height: 86rpx;
  border-bottom: 1px solid  #E5E5E5;
  background-color: #FFF;
  box-sizing: border-box;
}
.class-content .swiper-contents .class-type .type-sub.active {
  color: #00A9FF;
}
.class-content .swiper-contents .class-type .type-sub .t {
  position: relative;
  display: inline-block;
  overflow: initial;
}
.class-content .swiper-contents .class-type .type-sub .t .i {
  position: absolute;
  top: 25rpx;
  left: -50rpx;
  width: 40rpx;
  height: 40rpx;
  background-size: contain;
}
.class-content .swiper-contents .class-type .type-sub.video .i {
  background-image: url("data:image/png;base64,");
}
.class-content .swiper-contents .class-type .type-sub.video.active .i {
  background-image: url("data:image/png;base64,");
}
.class-content .swiper-contents .class-type .type-sub.live .i {
  background-image: url("data:image/png;base64,");
}
.class-content .swiper-contents .class-type .type-sub.live.active .i {
  background-image: url("data:image/png;base64,");
}
.class-content .swiper-contents .class-type .type-sub.face .i {
  background-image: url("data:image/png;base64,");
}
.class-content .swiper-contents .class-type .type-sub.face.active .i {
  background-image: url("data:image/png;base64,");
}
.class-content .swiper-contents .cols {
  margin-top: 10px;
  background-color: #FFF;
  border-bottom: 1px solid  #E5E5E5;
}
.class-content .swiper-contents .cols:first-child {
  margin-top: 0;
}
.class-content .swiper-contents .cols .packagebox {
  margin-bottom: 20rpx;
  border-bottom: 1px solid  #E5E5E5;
}
.class-content .swiper-contents .cols .c_title {
  font-size: 16px;
}
.class-content .swiper-contents .cols .c_title .infopack {
  padding-top: 10rpx;
}
.class-content .swiper-contents .cols .c_title .infopack .info_left {
  display: inline-block;
  padding-right: 20rpx;
  font-size: 24rpx;
  color: #939BA5;
}
.class-content .swiper-contents .cols .c_title .infopack .info_right {
  position: relative;
  display: inline-block;
  padding-left: 25rpx;
  font-size: 28rpx;
  color: #FD5151;
}
.class-content .swiper-contents .cols .c_title .infopack .info_right:after {
  content: '￥';
  position: absolute;
  left: 0;
  bottom: 0;
  font-size: 80%;
}
.class-content .swiper-contents .cols .c_title .infopack .info_right.free {
  color: #00A9FF;
}
.class-content .swiper-contents .cols .c_title .infopack .info_right.free:after {
  content: '';
  position: absolute;
  left: 0;
  bottom: 0;
}
.class-content .swiper-contents .cols .c_list {
  width: 104%;
}
.class-content .swiper-contents .cols .c_list .c_box {
  display: inline-block;
  width: 50%;
  padding: 10px 30rpx 0 0;
  box-sizing: border-box;
}
.class-content .swiper-contents .cols .c_list .c_box .img {
  width: 100%;
  height: 200rpx;
  border-radius: 10rpx;
  background-color: #DDD;
}
.class-content .swiper-contents .cols .c_list .c_box .img img {
  width: 100%;
}
.class-content .swiper-contents .cols .c_list .c_box .c_box_t {
  height: 2.2em;
  margin: 10rpx 0 0;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  text-overflow: ellipsis;
  word-break: break-all;
  line-height: 1.2;
  font-size: 28rpx;
  overflow: hidden;
}
.class-content .swiper-contents .classTitle {
  padding-bottom: 10px;
  font-size: 32rpx;
  color: #333;
  line-height: 22px;
}
.class-content .swiper-contents .info {
  margin-bottom: 30rpx;
  padding-bottom: 30rpx;
  border-bottom: 1px solid #EDEDED;
  overflow: hidden;
}
.class-content .swiper-contents .info .half {
  display: inline-block;
  padding-right: 20px;
  font-size: 24rpx;
  color: #939BA5;
}
.class-content .swiper-contents .other {
  padding-bottom: 10px;
}
.class-content .swiper-contents .other .price,
.class-content .swiper-contents .other .times {
  position: relative;
  display: inline-block;
  width: 50%;
  box-sizing: border-box;
}
.class-content .swiper-contents .other .price {
  padding-left: 26rpx;
  font-size: 36rpx;
  color: #FD5151;
}
.class-content .swiper-contents .other .price:after {
  content: '￥';
  position: absolute;
  bottom: 4rpx;
  left: 0;
  font-size: 24rpx;
  color: #FD5151;
}
.class-content .swiper-contents .other .price.free {
  color: #00C08B;
  padding-left: 0;
}
.class-content .swiper-contents .other .price.free:after {
  content: '';
  position: absolute;
  bottom: 4rpx;
  left: 0;
  font-size: 24rpx;
  color: #FD5151;
}
.class-content .swiper-contents .other .times {
  font-size: 24rpx;
  text-align: right;
  line-height: 36rpx;
}
.class-content .swiper-contents .classinfo {
  padding-bottom: 20px;
}
.class-content .swiper-contents .classinfo .title {
  margin-bottom: 8px;
  padding: 0 5px 0 8px;
  font-size: 32rpx;
  border-left: 3px solid  #0AF;
}
.class-content .swiper-contents .classinfo .swiper_tabs_content {
  font-size: 14px;
}
