@keyframes showpage {
  0% {
    opacity: 0;
    transform: translateY(300px);
  }
  60% {
    opacity: 1;
    transform: translateY(-10px);
  }
  80% {
    transform: translateY(5px);
  }
  100% {
    transform: translateY(0);
  }
}
@keyframes fadeIn {
  0% {
    opacity: 0;
  }
  80% {
    opacity: 0;
  }
  100% {
    opacity: 1;
  }
}
page {
  position: relative;
  min-height: 100%;
  background-color: #FFF;
}
.show {
  animation: 0.8s ease-out backwards;
  animation-name: showpage;
}
.member-login {
  margin: 10px 35px 0;
}
.member-login .title {
  padding-top: 80rpx;
  text-align: center;
  font-size: 46rpx;
}
.member-login .section {
  position: relative;
  padding: 26px 0 8px;
  border-bottom: 1px solid #CED1D3;
}
.member-login .section .section_input {
  width: 100%;
  font-size: 32rpx;
}
.member-login .section .section_input.code {
  width: 110px;
}
.member-login .section .section_input .input-placeholder {
  font-size: 32rpx;
  color: #D2D5D7;
}
.member-login .section .showpassword {
  position: absolute;
  right: 10rpx;
  bottom: 30rpx;
  width: 35rpx;
  height: 35rpx;
  background-image: url("data:image/png;base64,");
  background-size: contain;
  z-index: 9;
}
.member-login .section .showpassword.hide {
  background-image: url("data:image/png;base64,");
}
.member-login .section .clearinput {
  position: absolute;
  right: 10rpx;
  bottom: 25rpx;
  width: 35rpx;
  height: 35rpx;
  background-image: url("data:image/png;base64,");
  background-size: contain;
  z-index: 9;
}
.member-login .section .sendsms {
  position: absolute;
  top: 25px;
  right: 0;
  z-index: 1;
}
.login_btn {
  padding: 20px 35px 10px;
}
.login_btn .button {
  width: 100%;
  height: 95rpx;
  color: #FFF;
  background-color: #C9CBCF;
  font-size: 34rpx;
  line-height: 95rpx;
  text-align: center;
  border-radius: 5px;
}
.login_btn .button.active {
  background-color: #0AF;
}
.login_btn .long {
  width: 100%;
  padding-top: 10px;
  font-size: 28rpx;
}
.login_btn .long .primary {
  display: inline-block;
  color: #0AF;
}
.login_btn .left {
  display: inline-block;
  width: 50%;
  padding-top: 10px;
  font-size: 28rpx;
  text-align: left;
  color: #939BA5;
}
.login_btn .left.disabled {
  color: #939BA5;
}
.login_btn .right {
  display: inline-block;
  float: right;
  width: 50%;
  padding-top: 10px;
  font-size: 28rpx;
  text-align: right;
  color: #0AF;
}
.footer_btn {
  position: absolute;
  right: 0;
  bottom: 80px;
  left: 0;
  font-size: 32rpx;
  text-align: center;
  animation: 1s ease-out backwards;
  animation-name: fadeIn;
}
.footer_btn .text {
  display: inline-block;
  color: #333;
}
.footer_btn .primary {
  display: inline-block;
  color: #0AF;
}
