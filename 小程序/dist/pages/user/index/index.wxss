/*
    会员页面样式
*/
page {
  position: relative;
  padding-bottom: 60px;
  background-color: #F3F5F7;
}
.member {
  position: relative;
  min-height: 270rpx;
  max-height: 312rpx;
  margin-bottom: 30rpx;
  padding-top: 40rpx;
  background-color: #FFF;
  background-image: url("data:image/jpeg;base64,");
  box-sizing: border-box;
  border-top: 1px solid  #F3F5F7;
}
.member .setbtn {
  position: absolute;
  top: 10px;
  right: 20px;
  width: 20px;
  height: 20px;
  background-image: url("data:image/png;base64,");
  background-size: contain;
  display: none;
}
.member .pic {
  width: 120rpx;
  height: 120rpx;
  margin: 0 auto 10px;
  border-radius: 50px;
  overflow: hidden;
}
.member .pic image {
  width: 120rpx;
  height: 120rpx;
}
.member .name {
  margin-bottom: 8px;
  font-size: 36rpx;
  text-align: center;
  color: #333;
}
.member .slogen {
  padding: 5px 6%;
  text-align: center;
  font-size: 24rpx;
  color: #939CA5;
}
.menus {
  position: relative;
  margin-bottom: 30rpx;
  overflow: hidden;
}
.menus .menu_item {
  position: relative;
  padding: 20px 0 20px 60px;
  background-color: #FFF;
  font-size: 16px;
}
.menus .menu_item:after {
  content: '';
  position: absolute;
  top: 12px;
  left: 10px;
  width: 40px;
  height: 40px;
  background-size: contain;
}
.menus .menu_item:before {
  content: " ";
  position: absolute;
  top: 23px;
  right: 15px;
  display: inline-block;
  height: 10px;
  width: 10px;
  border-width: 4rpx 4rpx 0 0;
  border-color: #95B3E0;
  border-style: solid;
  transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
}
.menus .menu_item.class:after {
  background-image: url("data:image/png;base64,");
}
.menus .menu_item.package:after {
  background-image: url("data:image/png;base64,");
}
.menus .menu_item.order:after {
  background-image: url("data:image/png;base64,");
}
.menus .menu_item .line {
  position: absolute;
  right: 0;
  bottom: 0;
  left: 60px;
  height: 0;
  border-bottom: 1px solid #EEE;
}
.menus.white_space {
  margin-top: 50rpx;
  padding-left: 30rpx;
  padding-right: 30rpx;
}
.menus.white_space .logout {
  padding: 30rpx 0;
  font-size: 32rpx;
  color: #00A9FF;
  text-align: center;
  background-color: #FFF;
  border: 1px solid #DCE0EB;
  border-radius: 10rpx;
}
