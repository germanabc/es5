.swiper-tab {
  position: fixed;
  top: 0;
  right: 0;
  left: 0;
  height: 50px;
  z-index: 9;
  background-color: #FFF;
  border-bottom: 1px solid  #DDD;
}
.swiper-tab .swiper-tab-list {
  display: inline-block;
  width: 25%;
  height: 50px;
  font-size: 14px;
  line-height: 50px;
  text-align: center;
  box-sizing: border-box;
  overflow: hidden;
}
.swiper-tab .swiper-tab-list .small {
  display: inline-block;
  padding-left: 10rpx;
  font-size: 24rpx;
  color: #666;
}
.swiper-tab .swiper-tab-list.active {
  color: #0AF;
  border-bottom: 1px solid  #0AF;
}
.list {
  padding-top: 60px;
  padding-bottom: 60px;
  background-image: url(data:image/jpeg;base64,);
  overflow: auto;
  z-index: 2;
}
.list.none {
  padding-top: 60px;
  padding-bottom: 0;
}
.list .box {
  position: relative;
  width: 100%;
  margin: 0 0 10px;
  padding: 10px 0;
  background-color: #FFF;
  border-bottom: 1px solid  #EEE;
  overflow: hidden;
}
.list .box .content {
  position: relative;
  width: 98%;
  height: 32px;
  line-height: 32px;
  margin: 0 1%;
  overflow: hidden;
}
.list .box .content .name {
  display: inline-block;
  font-size: 28rpx;
  width: 70px;
  color: #939CA5;
  text-align: right;
}
.list .box .content .value {
  display: inline-block;
  padding-left: 20px;
  font-size: 28rpx;
  color: #333;
}
.list .box .content .value.danger {
  color: #FE5151;
}
.list .box .content .value.primary {
  color: #0AF;
}
