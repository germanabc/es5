/*
全局样式
*/
page {
  margin: 0;
  padding: 0;
  font-family: "Helvetica Neue", "PingFang SC", Helvetica, Arial, "Hiragino Sans GB", "Microsoft Yahei", "微软雅黑", sans-serif;
  background-color: #EBEBEB;
}
.toolbar {
  position: fixed;
  right: 0;
  bottom: 0;
  left: 0;
  height: 60px;
  background-color: #FFF;
  border-top: 1px solid #DFDFDF;
  z-index: 9;
  box-sizing: border-box;
}
.toolbar .menu {
  position: relative;
  display: inline-block;
  width: 25%;
  height: 60px;
  font-size: 28rpx;
  text-align: center;
}
.toolbar .menu.home .text:after {
  background-image: url("data:image/png;base64,");
}
.toolbar .menu.home.active .text:after {
  background-image: url("data:image/png;base64,");
}
.toolbar .menu.class .text:after {
  background-image: url("data:image/png;base64,");
}
.toolbar .menu.class.active .text:after {
  background-image: url("data:image/png;base64,");
}
.toolbar .menu.package .text:after {
  background-image: url("data:image/png;base64,");
}
.toolbar .menu.package.active .text:after {
  background-image: url("data:image/png;base64,");
}
.toolbar .menu.my .text:after {
  background-image: url("data:image/png;base64,");
}
.toolbar .menu.my.active .text:after {
  background-image: url("data:image/png;base64,");
}
.toolbar .menu.active {
  color: #0AF;
}
.toolbar .rec_url {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 1;
}
.toolbar .navigator-hover {
  background-color: transparent;
}
.toolbar .text {
  position: absolute;
  right: 0;
  bottom: 8px;
  left: 0;
  text-align: center;
}
.toolbar .text:after {
  content: '';
  position: absolute;
  top: -25px;
  left: 50%;
  width: 26px;
  height: 26px;
  margin-left: -13px;
  background-size: contain;
}
/* 没有内容样式 */
.nothing {
  padding-top: 30px;
  padding-bottom: 30px;
  background-color: #FFF;
  text-align: center;
  font-size: 14px;
  color: #666;
}
